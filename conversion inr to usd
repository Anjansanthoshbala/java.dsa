//Input currency in rupees and output in USD.
import java.util.*;
public class Main{
    public static void main(String args[]){
        Scanner sc= new Scanner(System.in);
        System.out.println("Enter the currency in rupees (inr): ");
        double inr = sc.nextInt();
        double conversionRate = 83.0;
         double  usd = inr/conversionRate;
         System.out.println(" equilavalent rupees usd = " + usd);
    }
}
